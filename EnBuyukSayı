import java.lang.reflect.Array;
import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class BiggerNumberOnArray {


    public static void main(String[] args) {

        Scanner sc1 = new Scanner(System.in);
        System.out.println("How many number do you want in your array?");
        System.out.println("Listede kaç eleman istediğini yaz");
        int numberOfArray = sc1.nextInt();
        List<Integer> theArray = new ArrayList<Integer>();
        for (int i = 0; i < numberOfArray; i++) {
            System.out.println("Write one by one");
            System.out.println("Sırayla yazınız");
            theArray.add(sc1.nextInt());
        }
        System.out.println(enBuyuk(theArray,numberOfArray));


    }
    public static int enBuyuk(List<Integer> dizi,int numberOfArray){
        int temp=0;
        for (int j = 0; j <numberOfArray; j++) {
            if(dizi.get(j)>temp){
                temp=dizi.get(j);
            }
        }
        return temp;
    }
}
